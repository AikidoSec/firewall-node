{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["oxc", "typescript", "unicorn", "node", "promise"],
  "env": {
    "browser": false,
    "node": true
  },
  "settings": {},
  "rules": {
    "no-console": "error",
    "no-empty": "error",
    "no-proto": "error",
    "no-regex-spaces": "error",
    "eqeqeq": "error",
    "no-prototype-builtins": "error",
    "require-await": "error"
  },
  "ignorePatterns": ["**/*.test.ts", "**/*.tests.ts"]
}
